<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor name="appdirect-connector-maven-archetype">
	<requiredProperties>
		<requiredProperty key="dbName">
		</requiredProperty>
	</requiredProperties>
	<fileSets>
		<fileSet packaged="true" filtered="true" encoding="UTF-8">
			<directory>src/main/java/</directory>
			<includes>
				<include>**/*.java</include>
			</includes>
			<exclude>check-deploy-integrity.sh</exclude>
		</fileSet>
		<fileSet packaged="false" filtered="true" encoding="UTF-8">
			<directory></directory>
			<excludes>
				<exclude>**/*.java</exclude>
				<exclude>**/handlers</exclude>
				<exclude>**/security</exclude>
				<exclude>check-deploy-integrity.sh</exclude>
			</excludes>
		</fileSet>
		<fileSet packaged="false" filtered="false" encoding="UTF-8">
			<directory></directory>
			<includes>
				<include>*.sh</include>
			</includes>
		</fileSet>
	</fileSets>
</archetype-descriptor>
